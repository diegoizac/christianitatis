# Configurações do npm para o deploy na Vercel

# Ignorar scripts durante a instalação
ignore-scripts=true

# Ignorar erros de peer dependencies
legacy-peer-deps=true

# Ignorar erros de engine
engine-strict=false

# Usar o registro npm padrão
registry=https://registry.npmjs.org/

# Desativar verificações de integridade
verify-store-integrity=false

# Desativar auditorias
audit=false

# Desativar atualizações
update-notifier=false

# Desativar progresso
progress=false

# Desativar logs
loglevel=error